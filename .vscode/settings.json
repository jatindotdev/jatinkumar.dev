  "biome.lspBin": "./node_modules/@biomejs/biome/bin/biome",
  "prettier.enable": false,
  "eslint.enable": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "biome.enabled": true,
  "typescript.tsdk": "node_modules/typescript/lib"
}
